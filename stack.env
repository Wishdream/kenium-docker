# ------ Lavalink node settings ------
# Name of the Lavalink node (this is not the hostname, just the name of the node)
NODE_NAME=main

# Port Lavalink listens on
NODE_PORT=2333

# Password of the Lavalink server (must match between Kenium and Lavalink)
NODE_PASSWORD=supersecret-password-here

# Use secure (HTTPS) connection? true/false
NODE_SECURE=false


# ------ Kenium bot credentials / runtime flags ------
# Discord bot token (DO NOT SHARE)
token=YOUR_DISCORD_BOT_TOKEN_HERE

# Discord bot client ID
id=YOUR_BOT_CLIENT_ID_HERE

# Enable prefix-based commands? (requires message content intent)
PREFIX_ENABLED=false

# Enable internal anti-docker process management?
ANTI_DOCKER=true
